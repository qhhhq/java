.log-list {
  display: flex;
  flex-direction: column;
  padding: 40rpx;
}
.log-item {
  margin: 10rpx;
}
.prd-info{
  background-color: #ccc;
  color: #fff;
  font-size: 36rpx;
  text-align: center;
  padding: 10rpx 0rpx;
}
.prd-img{
  width: 200rpx;
  height: 200rpx;
  border-radius: 400rpx;
}
.prd-detail{
  background-color: #fff;
  color: #000;
  font-size: 36rpx;
  padding: 20rpx;
  margin-bottom: 1rpx;
}