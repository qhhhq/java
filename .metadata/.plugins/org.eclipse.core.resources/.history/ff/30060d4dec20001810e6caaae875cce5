package net.qhhhq.utils;

import net.qhhhq.service.common.SysHead;

public class Utils {

	public static boolean theService(SysHead sysHead, String service) {
		boolean result = false;
		if((sysHead.getServiceCode()+sysHead.getMessageType()+sysHead.getMessageCode()).equals(service)) {
			result = true;
		}
		return result;
	}