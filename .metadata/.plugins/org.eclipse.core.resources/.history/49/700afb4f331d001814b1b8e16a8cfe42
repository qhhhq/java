package net.qhhhq.global.auth.factory;

import java.io.IOException;

import javax.xml.bind.JAXBException;

import net.qhhhq.global.auth.config.AuthList;
import net.qhhhq.utils.XmlUtils;

public class AuthFactory {

	private static AuthList list;

	public static void createAuthList(String xmlFile) {
		AuthList list = null;
		try {
			Object object = XmlUtils.xmlToBean(xmlFile,AuthList.class);
			list = (AuthList)object;
		} catch (JAXBException e) {
			e.printStackTrace();
		} catch (IOException e) {
			e.printStackTrace();
		}
	}
}
