package net.qhhhq.global.auth.config;

import java.util.List;
import java.util.concurrent.CopyOnWriteArrayList;

public class AuthConfig {

	private List<AuthServiceConfig> trans;
	private String serviceCode;
	private String messageType;
	private String messageCode;
	private static AuthConfig ac = new AuthConfig();

	public AuthConfig() {
		trans = new CopyOnWriteArrayList<AuthServiceConfig>();
	}

	public static AuthConfig getInstance() {
		return ac;
	}

	public List<AuthServiceConfig> getTrans() {
		return trans;
	}
	public void setTran(AuthServiceConfig asc) {
		this.trans.add(asc);
	}
	public String getServiceCode() {
		return serviceCode;
	}
	public void setServiceCode(String serviceCode) {
		this.serviceCode = serviceCode;
	}
	public String getMessageType() {
		return messageType;
	}
	public void setMessageType(String messageType) {
		this.messageType = messageType;
	}
	public String getMessageCode() {
		return messageCode;
	}
	public void setMessageCode(String messageCode) {
		this.messageCode = messageCode;
	}

}
